                                Console.Out.WriteLine();
                                Console.Out.WriteLine(">>>>>>>>>>>>>>>>>>>>>>>>>>>>>> Press any button <<<<<<<<<<<<<<<<<<<<<<");
                                Console.Out.WriteLine();
                                Console.Out.WriteLine();
                                Console.In.ReadLine();

                                J = (L - 273) * 4 / 5;
                                Console.Out.WriteLine();
                                Console.Out.WriteLine("Suhu konversi dari " + A + " ke Reamur adalah ( " + L + " - 273 ) x 4/5 = " + J);
                                Console.Out.WriteLine("Jadi " + L + " " + A + " = " + J + " Reamur");
                                Console.Out.WriteLine("Hasil konversi dari " + A + " ke Reamur adalah ");
                                Console.Out.WriteLine(J);
                                Console.Out.WriteLine();
                                Console.Out.WriteLine();
                                Console.Out.WriteLine(">>>>>>>>>>>>>>>>>>>>>>>>>>>>>> Press any button <<<<<<<<<<<<<<<<<<<<<<");
                                Console.Out.WriteLine();
                                Console.Out.WriteLine();
                                Console.In.ReadLine();
                            OO:

                                Console.Out.WriteLine("Apakah anda ingin melanjutkan ke proses konversi dari awal lagi ? (Ya / Tidak) ");
                                Console.Out.Write("Jawaban Anda : ");
                                O = Console.In.ReadLine();
                                Console.Out.WriteLine();
                                Console.Clear();
                                if (O == "Ya" || O == "YA" || O == "Y" || O == "y" || O == "Iya" || O == "IYA" || O == "iYA")
                                {
                                    goto Awal;
                                }
                                else if (O == "Tidak" || O == "TIDAK" || O == "tIDAK" || O == "tidak" || O == "GK" || O == "Gk" || O == "gK" || O == "gk" || O == "G" || O == "g")
                                {
                                    Console.Out.WriteLine("Terimakasih telah menggunakan program ini ");
                                    Console.Out.WriteLine("Mohon maaf jika ada kesalahan dalam kosa kata atau di sistem ini sendiri");
                                    Console.Out.WriteLine();
                                    Console.Out.WriteLine();
                                    Console.Out.WriteLine("Tekan Enter untuk melanjutkan ");
                                    Console.In.ReadLine();
                                    Console.Out.WriteLine("Tekan tombol apapun untuk keluar dari program ini");


                                }
                                else
                                {
                                    Console.Out.WriteLine("Jawablah pertanyaan tersebut dengan (Ya / Tidak )");
                                    Console.Out.WriteLine();
                                    Console.Out.WriteLine("Tekan Enter untuk melanjutkan ");
                                    Console.In.ReadLine();
                                    Console.Out.WriteLine();
                                    Console.Clear();
                                    goto OO;
                                }
                            }
                            else
                            {
                                Console.Out.WriteLine("Jawablah pertanyaan tersebut dengan (Ya / Tidak / Teruskan )");
                                Console.Out.WriteLine();
                                Console.Clear();
                                goto oo;
                            }
                        }
                        else
                        {
                            Console.Out.WriteLine("Jawablah pertanyaan tersebut dengan pilihan yang ada ( Reamur / Fahrenheit )");
                            Console.Out.WriteLine();
                            Console.Out.WriteLine("Tekan Enter untuk melanjutkan ");
                            Console.In.ReadLine();
                            Console.Out.WriteLine();
                            Console.Clear();
                            goto LORO;

                        }
                    }
                    else
                    {
                        Console.Out.WriteLine("Jawablah pertanyaan tersebut dengan (Ya / Tidak / Teruskan )");
                        Console.Out.WriteLine();
                        Console.Out.WriteLine("Tekan Enter untuk melanjutkan ");
                        Console.In.ReadLine();
                        Console.Out.WriteLine();
                        goto loro;
                    }
                }
                else
                {
                    Console.Out.WriteLine("Masukkan data dengan Celcius, Reamur, atau Fahrenheit");
                    Console.Out.WriteLine();
                    Console.Out.WriteLine("Tekan Enter untuk melanjutkan ");
                    Console.In.ReadLine();
                    Console.Clear();
                    goto Kalo;

                }
            }
            else
            {

                Console.Out.WriteLine("Masukkan data dengan Celcius, Reamur, Fahrenheit, atau Kelvin");
                Console.Out.WriteLine();

                Console.Out.WriteLine("Tekan Enter untuk melanjutkan ");
                Console.In.ReadLine();
                Console.Clear();

                Console.Out.WriteLine("Apakah anda ingin melanjutkan ke proses konversi dari awal lagi ? (Ya / Tidak) ");
            Mulai:
                Console.Out.Write("Jawaban Anda : ");
                G = Console.In.ReadLine();
                Console.Out.WriteLine();
                Console.Clear();
                if (G == "Ya" || G == "YA" || G == "Y" || G == "y" || G == "Iya" || G == "IYA" || G == "iYA")
                {
                    goto Awal;
                }
                else if (G == "Tidak" || G == "TIDAK" || G == "tIDAK" || G == "tidak" || G == "GK" || G == "Gk" || G == "gK" || G == "gk" || G == "G" || G == "g")
                {
                    Console.Out.WriteLine("Terimakasih telah menggunakan program ini ");
                    Console.Out.WriteLine("Mohon maaf jika ada kesalahan dalam kosa kata atau di sistem ini sendiri");
                    Console.Out.WriteLine("\n\n");
                    Console.Out.WriteLine("Tekan Enter untuk melanjutkan ");
                    Console.In.ReadLine();
                    Console.Out.WriteLine("Tekan tombol apapun untuk keluar dari program ini");

                }
                else
                {

                    Console.Out.WriteLine("Jawablah pertanyaan tersebut dengan (Ya / Tidak ");
                    Console.Out.WriteLine();
                    Console.Out.WriteLine("Tekan Enter untuk melanjutkan ");
                    Console.In.ReadLine();

                    Console.Clear();
                    Console.Out.WriteLine();
                    goto Mulai;
                }
            }
            Console.ReadKey();
        }

    }
}
